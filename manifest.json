{
  "name": "Send to Feedbin",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Send pages to Feedbin to read later.",
  "icons": {
    "16": "icons/feedbin_16.png",
    "32": "icons/feedbin_32.png",
    "48": "icons/feedbin_48.png",
    "128": "icons/feedbin_128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true,
    "browser_style": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "send-to-firefox@nickgravelyn.com"
    }
  }
}
